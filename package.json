{
  "name": "libox",
  "version": "0.3.0",
  "description": "A CLI tool to create library.",
  "bin": {
    "libox": "bin/libox.js"
  },
  "scripts": {
    "lint": "eslint '{bin,lib}/**/*.js'",
    "lint:fix": "eslint --fix '{bin,lib}/**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "cli",
    "template"
  ],
  "author": "heynext <heynext.xyz@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "chalk": "^3.0.0",
    "commander": "^4.0.1",
    "download-git-repo": "^3.0.2",
    "ejs": "^3.0.1",
    "fs-extra": "^8.1.0",
    "inquirer": "^7.0.0"
  },
  "devDependencies": {
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "{bin,lib}**/*.js": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/snokier/libox.git"
  },
  "bugs": {
    "url": "https://github.com/snokier/libox/issues"
  },
  "homepage": "https://github.com/snokier/libox",
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=6.0.0"
  }
}
